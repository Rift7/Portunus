{
    "manifest_version": 2,
    "name": "Portunus",
    "version": "1.0.1",
    "description": "The API Key Collector",
    "icons": {
        "96": "assets/icons/icon-96.png",
        "128": "assets/icons/icon-128.png",
        "512": "assets/icons/icon-512.png"
    },
    "permissions": [
        "activeTab",
        "storage",
        "webNavigation",
        "<all_urls>"
    ],
    "browser_action": {
        "default_icon": "assets/icons/icon-128.png",
        "default_popup": "popup.html",
        "default_title": "Portunus"
    },
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "background": {
        "scripts": [
            "assets/js/background.js"
        ]
    },
    "content_security_policy": "script-src 'self'; object-src 'self'",
    "web_accessible_resources": [
        "assets/icons/copy-96.png",
        "assets/icons/delete-96.png"
    ]
}
